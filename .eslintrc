{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended"
    ],
    "globals": {
        "browser": true,
        "page": true,
        "wp": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error", {
                "varsIgnorePattern": "^h$" 
            }
        ]
    }
}